{
  "ConnectionStrings": {
      "DefaultConnection": "server={{DbServer}};port=3306;database={{DbSchema}};uid={{DbUsername}};password={{DbPassword}};persistsecurityinfo=True;"
    //   "DefaultConnection": "server=localhost;userid=admin;password=123456;database=webApiDb;Allow User Variables=True"

  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "Tokens": {
    "Key": "0123456789ABCDEF",
    "Issuer": "http://localhost:48275"
  },
  "AllowedOrigins": {
    "Origins": "http://localhost:4200" //Insert your web app URL here for avoiding CORS errors
  }
}
